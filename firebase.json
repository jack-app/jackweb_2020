{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects":[
      {
        "source": "/",
        "destination":"https://jack-website.netlify.app/",
        "type": 301
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
